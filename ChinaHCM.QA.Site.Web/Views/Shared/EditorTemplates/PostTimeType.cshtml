@using ChinaHCM.QA.Core.Model
@model PostTimeType
           
@foreach (int value in Enum.GetValues(typeof(PostTimeType)))
{
    <span>
    @Html.RadioButton(Enum.GetName(Model.GetType(), value), value, value == (int)Model, new { @Name = "PostTime" }) 
    @GlobalConst.PostTimeTypeName[value]
    </span>
}
